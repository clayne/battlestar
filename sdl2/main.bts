const initsdl = "Initializing the SDL 2 video subsystem..."
const sleep2 = "Sleeping for 2 seconds..."
const done = "done.", 10

extern SDL_Init
extern SDL_Delay

fun main
    // Initialize the SDL2 Video subsystem
    write(initsdl)
    a = 0x20
    funparam[0] = a
    call SDL_Init
    write(done)

    // Wait 2 seconds
    write(sleep2)
    a = 2000
    funparam[0] = a
    call SDL_Delay
    write(done)
end

