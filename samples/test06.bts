const commence = "Commence jump prep.", 10
const nebula = "We're going to the ionian nebula.", 10
const hi = "Hi!", 10

fun hello
    write(commence)
    write(nebula)
end

fun main
    write(hi)

    // "call hello", "hello" and "hello()" does the same thing
    call hello
    hello
    hello()
end

