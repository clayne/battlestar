const twice = "Twice", 10
const thrice = "Thrice", 10
const once = "Once", 10
const nooo = "Nooo!", 10

fun main
    loop 17
        print(once)
        break        // Break out of loop
    end
    loop 2 
        print(twice) // This happens two times
    end
    loop 3
        print(thrice)
        continue     // Continue loop from top
        print(nooo)  // This never happens
    end
end

