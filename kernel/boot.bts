bootable                       // this is not a regular source file, but a kernel

extern kernel_main             // the C function that is called when booting

fun main
    kernel_main
    halt                       // clear interrupts, halt and loop forever
end

fun success
    halt                       // Not quite sure how to indicate success here
end

void kernel_main() {
    success();
}

